    "version": "2.0.0",
    "tasks": [
        {
            "label": "CMake Build Debug",
            "command": "cmake --build ${workspaceFolder}/build/Debug",
            "type": "shell",
            "group": "build",
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "type": "cppbuild",
